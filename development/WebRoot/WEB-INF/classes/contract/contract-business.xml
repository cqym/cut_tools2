			</property>
		</bean>
	  </property>
 </bean>
 <bean id="ContractOutExcel"
		class="com.tl.resource.business.contract.ContractOutExcelImp">
			<property name="contractInforDAO">
				<ref bean="contractInforDAO" />
			</property>
			<property name="contractProductDetailDAO">
