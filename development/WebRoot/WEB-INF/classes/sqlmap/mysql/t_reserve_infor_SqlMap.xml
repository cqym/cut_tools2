   <!-- 根据分页参数获取公司信息 -->
   <select id="getReserveByPage" parameterClass="java.util.Map" resultMap="ibatorgenerated_BaseResultMap">
   	 <![CDATA[
   	 SELECT b.id, c.product_code, c.brand_code ,c.leaf,c.product_name,c.product_unit,          
c.product_sort,c.product_brand,c.product_source     
,CASE WHEN b.amount IS NULL THEN 0 ELSE b.amount END AS amount,b.product_position,b.slave_file,b.memo,b.reserve_code,          
b.tools_id, b.parent_tools_id,b.price, b.currency_name FROM(
SELECT a.id, a.product_code, a.brand_code ,a.leaf,a.product_name,a.product_unit,          
a.product_sort_code AS product_sort,a.product_brand,a.product_source          
FROM t_product_tools_infor a where a.parent_id = 'root'   
       ]]>
      
      	<isNotEmpty prepend="and" property="productName">
				a.product_name like '%$productName$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="productCode">
