<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
	<action-mappings>
		<action path="/querystatistics/CustomerOrderQueryAction" scope="request" validate="false" parameter="m"></action>
		<action path="/querystatistics/OrderQueryAction" scope="request" validate="false" parameter="m"></action>
		<action path="/querystatistics/ProductsQueryAction" scope="request" validate="false" parameter="m"></action>
		<action path="/querystatistics/UserWorkQueryAction" scope="request" validate="false" parameter="m"></action>
	</action-mappings>
	<controller>
		<set-property property="processorClass"
			value="org.springframework.web.struts.DelegatingRequestProcessor" />
	</controller>
</struts-config>
